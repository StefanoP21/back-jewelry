{
	"name": "back-jewelry",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"prepare": "husky",
		"dev": "tsnd --respawn --clear src/app.ts",
		"build": "rimraf ./dist && tsc",
		"start": "npm run build && node dist/app.js",
		"test": "jest --verbose --maxWorkers=1 --testTimeout=15000",
		"test:watch": "jest --watch --maxWorkers=1",
		"test:coverage": "jest --coverage --maxWorkers=1",
		"lint": "eslint .",
		"format": "eslint . --fix",
		"prisma:generate": "npx prisma generate",
		"prisma:migrate": "npx prisma migrate dev",
		"prisma:migrate:deploy": "npx prisma migrate deploy",
		"prisma:migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
		"prisma:migrate:test:deploy": "dotenv -e .env.test -- npx prisma migrate deploy",
		"postinstall": "npx prisma generate"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@eslint/compat": "^1.1.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.11.1",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.13",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.5.5",
		"@types/supertest": "^6.0.2",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.57.1",
		"eslint-config-love": "^71.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.9.0",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.10",
		"prettier": "^3.3.3",
		"prisma": "^6.0.1",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0"
	},
	"dependencies": {
		"@prisma/client": "^5.22.0",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"env-var": "^7.5.0",
		"express": "^4.21.0",
		"express-rate-limit": "^7.4.0",
		"jsonwebtoken": "^9.0.2"
	}
}
